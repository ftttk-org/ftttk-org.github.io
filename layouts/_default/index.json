{{- $.Scratch.Add "index" slice -}}
{{- range site.RegularPages -}}
    {{- if and (not .Params.searchHidden) (ne .Layout `archives`) (ne .Layout `search`) -}}
        {{- $.Scratch.Add "index" (dict "title" .Title "date" .Date "content" .Plain "permalink" .Permalink "summary" .Summary "categories" (.Params.categories | default slice) "tags" (.Params.tags | default slice)) -}}
    {{- end -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}

